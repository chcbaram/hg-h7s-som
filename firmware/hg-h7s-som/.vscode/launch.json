    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Boot - ST-Link",
            "cwd": "${workspaceFolder}",
            "type": "cortex-debug",
            "executable": "${command:STM32VSCodeExtension.bootFlashBootTarget}",
            "symbolFiles": [
                "${command:STM32VSCodeExtension.bootFlashBootTarget}",
            ],
            "request": "launch",        
            "servertype": "stlink",
            "device": "STM32H7S7I8Kx",
            "interface": "swd",
            "serialNumber": "",        //Set ST-Link ID if you use multiple at the same time
            "runToEntryPoint": "main",
            "svdFile": "${config:STM32VSCodeExtension.cubeCLT.path}/STMicroelectronics_CMSIS_SVD/STM32H7S.svd",
            "serverpath": "${config:STM32VSCodeExtension.cubeCLT.path}/STLink-gdb-server/bin/ST-LINK_gdbserver",
            "stm32cubeprogrammer":"${config:STM32VSCodeExtension.cubeCLT.path}/STM32CubeProgrammer/bin",
            "stlinkPath": "${config:STM32VSCodeExtension.cubeCLT.path}/STLink-gdb-server/bin/ST-LINK_gdbserver",   
            "armToolchainPath": "${config:STM32VSCodeExtension.cubeCLT.path}/GNU-tools-for-STM32/bin",
            "gdbPath":"${config:STM32VSCodeExtension.cubeCLT.path}/GNU-tools-for-STM32/bin/arm-none-eabi-gdb",
            "v1": false,
            "serverArgs": [
                "-m", "1",
                "-t", "-s", 
            ],
            // "preLaunchTask": "Build + Flash in external Memory"
        },
        {
          "name": "Debug external memory - ST-Link",
          "cwd": "${workspaceFolder}",
          "type": "cortex-debug",
          "executable": "${command:STM32VSCodeExtension.bootFlashBootTarget}",
          "symbolFiles": [
              "${command:STM32VSCodeExtension.bootFlashAppliTarget}",
              "${command:STM32VSCodeExtension.bootFlashBootTarget}",
          ],
          "request": "launch",        
          "servertype": "stlink",
          "device": "STM32H7S7I8Kx",
          "interface": "swd",
          "serialNumber": "",        //Set ST-Link ID if you use multiple at the same time
          "runToEntryPoint": "main",
          "svdFile": "${config:STM32VSCodeExtension.cubeCLT.path}/STMicroelectronics_CMSIS_SVD/STM32H7S.svd",
          "serverpath": "${config:STM32VSCodeExtension.cubeCLT.path}/STLink-gdb-server/bin/ST-LINK_gdbserver",
          "stm32cubeprogrammer":"${config:STM32VSCodeExtension.cubeCLT.path}/STM32CubeProgrammer/bin",
          "stlinkPath": "${config:STM32VSCodeExtension.cubeCLT.path}/STLink-gdb-server/bin/ST-LINK_gdbserver",   
          "armToolchainPath": "${config:STM32VSCodeExtension.cubeCLT.path}/GNU-tools-for-STM32/bin",
          "gdbPath":"${config:STM32VSCodeExtension.cubeCLT.path}/GNU-tools-for-STM32/bin/arm-none-eabi-gdb",
          "v1": false,
          "serverArgs": [
              "-m", "1",
              "-t", "-s", 
              "-el", "${command:STM32VSCodeExtension.bootFlashExtMemLoaderTarget}.stldr" 
          ],
          "preLaunchTask": "Build + Flash in external Memory"
      },
      {
            "name": "Attach external memory - ST-Link",
            "cwd": "${workspaceFolder}",
            "type": "cortex-debug",
            "executable": "${command:STM32VSCodeExtension.bootFlashBootTarget}",
            "symbolFiles": [
                "${command:STM32VSCodeExtension.bootFlashAppliTarget}",
                "${command:STM32VSCodeExtension.bootFlashBootTarget}",
            ],
            "request": "attach",
            "servertype": "stlink",
            "device": "STM32H7S7I8Kx", //MCU used
            "interface": "swd",
            "serialNumber": "",        //Set ST-Link ID if you use multiple at the same time
            "runToEntryPoint": "main",
            "svdFile": "${config:STM32VSCodeExtension.cubeCLT.path}/STMicroelectronics_CMSIS_SVD/STM32H7S.svd",
            "serverpath": "${config:STM32VSCodeExtension.cubeCLT.path}/STLink-gdb-server/bin/ST-LINK_gdbserver",
            "stm32cubeprogrammer":"${config:STM32VSCodeExtension.cubeCLT.path}/STM32CubeProgrammer/bin",
            "stlinkPath": "${config:STM32VSCodeExtension.cubeCLT.path}/STLink-gdb-server/bin/ST-LINK_gdbserver",   
            "armToolchainPath": "${config:STM32VSCodeExtension.cubeCLT.path}/GNU-tools-for-STM32/bin",
            "gdbPath":"${config:STM32VSCodeExtension.cubeCLT.path}/GNU-tools-for-STM32/bin/arm-none-eabi-gdb",            
            "v1": false,               //Change it depending on ST Link version
            "serverArgs": [
                "-m","1",
            ],
            /* If you use external loader, add additional arguments */
            //"serverArgs": ["--extload", "path/to/ext/loader.stldr"],
        }
    ]
}